    <!--<wv2:WebView2 Name="wb1" Source="https://www.microsoft.com" />-->

    <eo:WebControl Margin="20 40" >
      <eo:WebControl.WebView>
        <eo:WebView x:Name="wb1" >
        </eo:WebView>
      </eo:WebControl.WebView>
    </eo:WebControl>

    <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" HorizontalAlignment="Center" >
      <Button Content="_Google" Click="Button_Click" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="10"/>
      <Button Content="_Linked" Click="Button_Clic2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="10" />
