<lnk:TopmostUnCloseableWindow Width="1000" Height="500" x:Class="AlexPi.Scr.Vws.ContainerK" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" 
                              Title="K - 11" 
                              xmlns:lnk="clr-namespace:AlexPi.Scr.Unclosables" 
                              xmlns:UsrCtrls="clr-namespace:AlexPi.Scr.UsrCtrls" 
                              xmlns:MSGraphSlideshow="clr-namespace:MSGraphSlideshow;assembly=MSGraphSlideshow" 
                              Style="{StaticResource MouseEnterableSlowShowWindow}">
  <WindowChrome.WindowChrome>
    <WindowChrome CaptionHeight="0" ResizeBorderThickness="15" />
