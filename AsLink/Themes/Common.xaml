  <Style x:Key="WindowStyle_Aav0" TargetType="{x:Type Window}" >
    <Setter Property="WindowStyle"        Value="None"        />
    <Setter Property="Background"         Value="Transparent" />
    <Setter Property="AllowsTransparency" Value="True"        />
    <Setter Property="ResizeMode"         Value="CanResize"   />
  </Style>

  <Style TargetType="Window" BasedOn="{StaticResource WindowStyle_Aav0}" x:Key="MouseOverMakesOpagueStyle">
    <Setter Property="Opacity" Value=".3"/>
    <Style.Triggers>
      <Trigger Property="IsMouseOver" Value="true">
        <Setter Property="Window.Opacity" Value=".9" />
      </Trigger>
    </Style.Triggers>
  </Style>

  <Style TargetType="TextBox" x:Key="MouseOverMakesOpagueTbxStyle">
    <Setter Property="BorderThickness"      Value="0" />
    <Setter Property="Background"           Value="Transparent" />
    <Style.Triggers>
      <Trigger Property="IsMouseOver"       Value="true">
        <Setter Property="Background"       Value="#ffd" />
        <Setter Property="TextBox.Opacity"  Value="1" />
      </Trigger>
      <Trigger Property="IsFocused"         Value="true">
        <Setter Property="Background"       Value="#ffa" />
        <Setter Property="TextBox.Opacity"  Value="1" />
      </Trigger>
    </Style.Triggers>
  </Style>

  <!--needs more work:-->
  <Style TargetType="TextBox" x:Key="FocusedMakesOpagueTbxStyle">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type TextBox}">
          <Border x:Name="border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
            <ScrollViewer x:Name="PART_ContentHost" Focusable="false" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden"/>
          </Border>
          <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="false">
              <Setter Property="Opacity" TargetName="border" Value="0.56"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="true">
              <Setter Property="BorderBrush" TargetName="border" Value="#0f0"/>
            </Trigger>

            <Trigger Property="IsFocused" Value="false">
              <Setter Property="TextBox.Opacity"  Value=".1" />
            </Trigger>
            <Trigger Property="IsFocused" Value="true">
              <Setter Property="Background"       Value="#ff8" />
              <Setter Property="TextBox.Opacity"  Value="1" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

