  <Style TargetType="{x:Type Slider}">
    <Style.Triggers>
      <EventTrigger RoutedEvent="Control.MouseEnter">
        <BeginStoryboard>
          <Storyboard >
            <DoubleAnimation Duration="0:0:.3" To=".51" Storyboard.TargetProperty="Opacity"/>
          </Storyboard>
        </BeginStoryboard>
      </EventTrigger>
      <EventTrigger RoutedEvent="Control.MouseLeave">
        <BeginStoryboard>
          <Storyboard >
            <DoubleAnimation Duration="0:0:.3" To="0.01" Storyboard.TargetProperty="Opacity"/>
          </Storyboard>
        </BeginStoryboard>
      </EventTrigger>
    </Style.Triggers>
  </Style>

